<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>NupkgWrench</id>
    <version>$version$</version>
    <authors>emgarten</authors>
    <owners>emgarten</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/emgarten/NupkgWrench/blob/master/LICENSE.md</licenseUrl>
    <projectUrl>https://github.com/emgarten/NupkgWrench/</projectUrl>
    <description>NupkgWrench is a command line tool to read and modify nupkgs.</description>
    <copyright>Copyright Â© 2017 Justin Emgarten</copyright>
    <tags>nupkg nuget-powertool</tags>
    <repository type="git" url="https://github.com/emgarten/NupkgWrench" branch="$branch$" commit="$commit$" />
    <developmentDependency>true</developmentDependency>
    <iconUrl>https://emgartenstatic.blob.core.windows.net/nupkgs/icons/nupkgwrench.png</iconUrl>
    <packageTypes>
      <packageType name="DotnetTool" />
    </packageTypes>
  </metadata>
  <files>
    <file src="..\..\artifacts\publish\*.exe" target="tools\" />
    <file src="..\..\artifacts\publish\netcoreapp2.1\*.dll" target="tools\netcoreapp2.1\any\" />
    <file src="..\..\artifacts\publish\netcoreapp2.1\DotnetToolSettings.xml" target="tools\netcoreapp2.1\any\" />
    <file src="..\..\artifacts\publish\netcoreapp2.1\NupkgWrench.runtimeconfig.json" target="tools\netcoreapp2.1\any\" />
    <file src="..\..\build\nupkgIncludes\NupkgWrench.props" target="build\NupkgWrench.props" />
  </files>
</package>